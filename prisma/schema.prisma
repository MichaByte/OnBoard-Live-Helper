generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model users {
  id             Int       @id @default(autoincrement())
  slack_user_id  String    @unique
  name           String?
  email          String?
  description    String
  is_approved    Boolean?  @default(false)
}
